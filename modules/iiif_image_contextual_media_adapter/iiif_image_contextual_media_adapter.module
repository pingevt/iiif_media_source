<?php

use Drupal\media_library_media_modify\Form\FileEditForm;

/**
 * Implements hook_media_source_info_alter().
 */
function iiif_image_contextual_media_adapter_media_source_info_alter(array &$sources): void {
  if (empty($sources['iiif_image']['forms']['media_library_media_modify_edit'])) {
    $sources['iiif_image']['forms']['media_library_media_modify_edit'] = FileEditForm::class;
  }
}
